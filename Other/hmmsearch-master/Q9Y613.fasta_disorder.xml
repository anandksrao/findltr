<script type="text/javascript">
  (function() {
    var ga = document.createElement('script');
    ga.type = 'text/javascript';
    ga.async = true;
    ga.src = ('https:'   == document.location.protocol ? 'https://ssl'   : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();

  $('#taxfilter_content').show();

  $('document').timeout = 45 * 1000;

  $(document).ready( function () {
    var params = window.location.search;
    var dataLoad = true;


    $('.domGraphics').each(function() {
      $(this).text('');
      var id = '#' + $(this).attr('id');
      var seq_id = id.replace('#dom_','seq_');
      var pg = new PfamGraphic();
      pg.setParent(id);
      pg.setSequence(window[seq_id]);
      pg.render();
      var new_width = $(this).parent().width();
      pg.resize( new_width );
    });

    if ($('#domGraph').length && typeof sequence !== 'undefined') {
      var pg = new PfamGraphic('#domGraph', sequence);
      pg.render();
      var new_width = $('#domGraph').parent().width();
      pg.resize( new_width );

      $("#hmmscan tbody tr:not('.alignment')")
        .bind('mouseover', function () {
          var uniq  = $(this).attr('data-uniq');
          pg.highlight({uniq:uniq});
        })
        .bind('mouseout', function () {
          var uniq  = $(this).attr('data-uniq');
          pg.highlight({uniq:uniq, status:'off'});
        });


      
    };

    if (params.match(/range=\d+,\d+/) && ! params.match(/l=1/)) {
      dataLoad = false;
    }

    var url = '/results//score';

    if ( dataLoad && $('#barGraph').length && typeof hitGraphData !== 'undefined'){
      $('#barGraph').distGraph();
    }

    $('#hmmscan').simpleTable();
    
      $('#hmmscan').alignmentToggle();
    
    $('#hmmscan').sortable();
    $('.meta').meta_toggle();
    $('#isajax').attr('value',1);


    

    if (typeof uuid !== 'undefined') {
      var page_num = null;
      if (typeof page !== 'undefined') {
        page_num = page;
      }
      if (typeof phmmerStatus !== 'undefined') {

        if (typeof annotationpfamaUUID === 'undefined') {
          annotationpfamaUUID = null;
        }

        $.loadPhmmer(uuid, phmmerStatus, page_num, annotationpfamaUUID );
      }
    }






    $('.formatchoices').addClass('fchoice');

    $('.fchoice div').not('.disabled').click(function(e){
      e.preventDefault();
      $('.fchoice div').removeClass('fselected');
      $(this).addClass('fselected').children('input').attr('checked', true)
      if ($('#job_details').length > 0) {
        $(this).parents('form').trigger('validate').children('.centered').hide();
      }
    });

  });

</script>
